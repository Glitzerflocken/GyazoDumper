{
	"name": "GyazoDumper",
	"version": "1.3.0",
	"manifest_version": 3,
	"key": "",
	"description": "Automatically saves newly created Gyazo Pictures into a Folder on your Hard Drive.",
	"icons": {
		"128": "GyazoDumper.png"
	},
	"content_scripts": [
		{
			"matches": [ "https://gyazo.com/*" ],
			"js": [ "Content.js" ],
			"run_at": "document_idle"
		}
	],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "SettingsPopup.html",
		"default_icon": "GyazoDumper.png"
	},
	"permissions": [
		"activeTab",
		"downloads",
		"storage",
		"nativeMessaging"
	],
	"web_accessible_resources": [
		{
			"resources": [ "GyazoDumper.png" ],
			"matches": [ "https://gyazo.com/*" ]
		}
	]
}